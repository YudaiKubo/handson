{
  "executionRoleArn": "arn:aws:iam::315408206347:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "kubo-test-nginx-container",
      "image": "<NGINX_IMAGE_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
    {
      "name": "kubo-test-php-container",
      "image": "<PHP_IMAGE_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
    {
      "name": "kubo-test-mysql-container",
      "image": "<MYSQL_IMAGE_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "kubo-test-task-definition"
}