{
  "executionRoleArn": "arn:aws:iam::950457927752:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hoge-container",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "hoge-task-definition"
}