{
  "executionRoleArn": "arn:aws:iam::315408206347:role/kubo-test-taskExecutionRole",
  "containerDefinitions": [
    {
      "name": "kubo-test-wordpress-container",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
            "name": "DB_URL",
            "valueFrom": "kubo-test-rds-url"
        }
    ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "kubo-test-task-definition"
}